export EDITOR=vim
alias notifyDone='terminal-notifier -title "Terminal" -message "Task complete!"'

alias del='git branch -d'
alias gb='git branch'
alias gba='git branch -a'
alias gc='git commit'
alias gco='git checkout'
alias gd='git diff | mate'
alias gfa='git fetch --all'
alias gl='git pull'
alias glog='git log --graph --oneline --decorate --date=relative --all --format="%h %<(150,trunc)%s"'
alias glom="git pull origin master"
alias gp='git push origin HEAD'
alias gpo='git push origin'
alias gpu='branch=$(git branch | grep "\*" | awk "{ print \$2 }"); git push --set-upstream origin $branch'
alias gst='git status'
